    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="108dp"
        android:layout_height="82dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/textView17"
